{
  "name": "workspace",
  "type": "module",
  "version": "0.7.2",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=22.7.0",
    "pnpm": ">=9.9.0",
    "npm": ">=10.8.0"
  },
  "scripts": {
    "postinstall": "npx simple-git-hooks",
    "release": "bumpp -r",
    "test": "pnpm run --filter vector test --coverage"
  },
  "devDependencies": {
    "bumpp": "^9.5.2",
    "lint-staged": "^15.2.9",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run --recursive lint --fix"
  }
}
